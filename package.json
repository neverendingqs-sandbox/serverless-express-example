{
  "name": "serverless-express-example",
  "version": "1.0.0",
  "description": "Serverless framework example with express.js.",
  "main": "server.js",
  "scripts": {
    "deploy:prod": "sls deploy --stage 'prod' --region $AWS_REGION",
    "deploy:dev": "sls deploy",
    "start": "nodemon server-local",
    "test": "mocha --recursive && eslint --ignore-path .gitignore . && sls deploy --noDeploy"
  },
