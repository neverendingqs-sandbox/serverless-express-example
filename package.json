  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/neverendingqs-sandbox/serverless-express-example/issues"
  },
  "homepage": "https://github.com/neverendingqs-sandbox/serverless-express-example#readme",
  "dependencies": {
    "body-parser": "^1.17.0",
    "connect-dynamodb": "^1.0.9",
    "csurf": "^1.9.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "serverless-http": "^1.4.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.16.1",
    "eslint-plugin-mocha": "^4.8.0",
